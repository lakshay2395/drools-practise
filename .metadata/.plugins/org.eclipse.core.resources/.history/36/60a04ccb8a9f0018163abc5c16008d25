package rules

import com.practise.drools.model.Employee;
import com.practise.drools.model.Department;

rule "Cross - Reference Example"

    when
        emp : Employee()
        dept : Department()
    then
        System.out.println("emp = "+emp+" , dept = "+dept);

end

